{
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "script-src 'self' https://challenges.cloudflare.com; frame-src https://challenges.cloudflare.com;"
                }
            ]
        }
    ]
}
